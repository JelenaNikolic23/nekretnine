    },
    "surface": {
      "type": "decimal"
    },
    "rooms": {
      "type": "integer"
    },
    "bathrooms": {
      "type": "integer"
    },
    "garages": {
      "type": "integer"
    },
    "balcons": {
      "type": "integer"
    },
    "cableNetwork": {
      "type": "boolean"
    },
    "keepingRoom": {
      "type": "boolean"
    },
    "parkingPlace": {
      "type": "boolean"
    },
    "sharedRoom": {
      "type": "boolean"
    },
    "pictures": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "floorNumber": {
      "type": "integer"
    },
    "legalized": {
